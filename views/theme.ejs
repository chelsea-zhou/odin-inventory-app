<div class="theme"> 
    <a href="/themes/<%= theme.id %>" style="text-decoration: none;">
        <!-- this src actually sends a request to server !! -->
        <img src="/images/<%= theme.cover_image%>">
        <p><%= theme.title %></p>
    </a>

    <button onClick="location.href='/themes/<%=theme.id%>/delete'">delete</button>
    <button onClick="location.href='/themes/<%=theme.id%>/update'">update</button>
</div>